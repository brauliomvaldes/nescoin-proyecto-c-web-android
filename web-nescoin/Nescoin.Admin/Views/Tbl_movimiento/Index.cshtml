@model IEnumerable<Nescoin.Conexion.Models.Tbl_movimiento>

@{
    ViewBag.Title = "Movimientos";
}

<h2>Movimientos</h2>

<p>
    @Html.ActionLink("Crear nuevo", "Create")
    <br />
    <a href="/Home/Index">Volver a administración</a>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.fecha_movimiento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.comentario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.estado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.num_ref_movimiento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tbl_Calificacion.descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tbl_Forma_Pago.descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tbl_Usuario.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tbl_tipo_movimiento.nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.fecha_movimiento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.comentario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.estado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.num_ref_movimiento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tbl_Calificacion.descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tbl_Forma_Pago.descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tbl_Usuario.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tbl_tipo_movimiento.nombre)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.id_movimiento }) |
            @Html.ActionLink("Detalle", "Details", new { id=item.id_movimiento }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.id_movimiento })
        </td>
    </tr>
}

</table>
